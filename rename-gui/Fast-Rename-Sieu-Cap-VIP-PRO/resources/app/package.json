{
  "name": "rename-gui",
  "version": "1.5.0",
  "description": "Premium Fast Rename Pro V3",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder --win"
  },
  "build": {
    "appId": "com.antigravity.fastrename",
    "productName": "Fast Rename Pro V3",
    "asar": true,
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": "dir"
    }
  },
  "devDependencies": {
    "electron": "^40.6.1",
    "electron-builder": "^26.8.1"
  },
  "dependencies": {
    "fs-extra": "^11.3.0",
    "sortablejs": "^1.15.7"
  }
}
